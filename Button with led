int led = 8;//pin 8

int button = 7;//pin 7

void setup() {

pinMode(led,OUTPUT);
pinMode(button,INPUT);
  
}

void loop() {

if(digitalRead(button)==HIGH)// press button
{
digitalWrite(led,HIGH);}//led on(1)

if(digitalRead(button)==LOW)//no press button
{
digitalWrite(led,LOW);}//led off(0)
  
}

